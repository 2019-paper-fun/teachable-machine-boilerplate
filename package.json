{
  "name": "cl-teachable-machine-boilerplate",
  "version": "0.1.0",
  "license": "",
  "dependencies": {
    "deeplearn": "^0.3.12",
    "deeplearn-knn-image-classifier": "^0.2.0",
    "@tensorflow-models/knn-classifier": "^0.2.2",
    "@tensorflow-models/mobilenet": "^0.2.2",
    "@tensorflow/tfjs": "^0.13.0",
    "autoless": "^0.1.7",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "concurrently": "^3.5.0",
    "pre-commit": "^1.2.2",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "start": "budo main.js:dist/build.js --live --host localhost",
    "build": "browserify main.js -o dist/build.js"
  },
  "pre-commit": [
    "build"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "syntax-async-functions",
            "transform-regenerator"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.0.16",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.0.15",
    "babelify": "^8.0.0",
    "budo": "^10.0.4"
  }
}
